<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4B568">

    <Button
        android:id="@+id/button"
        android:layout_width="305dp"
        android:layout_height="97dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="143dp"
        android:text="Informações do pet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.378"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="254dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="305dp"
        android:layout_height="97dp"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="143dp"
        android:text="Carteira de Vacinação"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.31"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="403dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="293dp"
        android:layout_height="572dp"
        app:srcCompat="@drawable/pet"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="58dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="308dp"
        android:layout_height="642dp"
        app:srcCompat="@drawable/cdvacina"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/patinha"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="-116dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="58dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/configuracao"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>